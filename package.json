{
  "name": "image-generation-platform",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev ",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "start:web": "cd ./apps/web && npm run start ",
    "start:backend": "cd ./apps/backend && npm run start",
    "generate:db": "npx prisma generate --schema=./packages/db/prisma/schema.prisma"
  },
  "devDependencies": {
    "@types/ws": "^8.18.1",
    "prettier": "^3.5.0",
    "turbo": "^2.4.1",
    "typescript": "5.7.3"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "bun@1.1.26",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "@clerk/express": "^1.7.62",
    "@clerk/nextjs": "^6.32.0",
    "@types/jsonwebtoken": "^9.0.10",
    "cloudinary": "^2.8.0",
    "dotenv": "^17.2.3",
    "download": "^8.0.0",
    "next": "^15.6.0-canary.8",
    "openai": "^4.90.0"
  }
}
